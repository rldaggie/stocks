    <tbody>
      <!-- ko foreach: groups -->
        <tr>
          <td colspan="6">&nbsp;</td>
        </tr>
        <!-- ko foreach: items -->
          <tr>
            <td data-bind="text: name"></td>
            <!-- ko foreach: values -->
              <td data-bind="text: $data" style="text-align: right;"></td>
            <!-- /ko -->
          </tr>
        <!-- /ko -->
      <!-- /ko -->
    </tbody>
  </table>
</div><!--// #<%= dom_id %> -->