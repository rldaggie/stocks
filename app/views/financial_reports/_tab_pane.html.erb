    <colgroup>
      <col class="span7">
      <col class="span1">
      <col class="span1">
      <col class="span1">
      <col class="span1">
      <col class="span1">
    </colgroup>
    <thead>
      <tr>
        <th>Period Ending</th>
        <!-- ko foreach: $root.periods -->
          <th data-bind="text: period" style="text-align: right;"></th>
        <!-- /ko -->
      </tr>
